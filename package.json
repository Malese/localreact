{
  "name": "localreact",
  "version": "0.0.1",
  "description": "A react playground https://scotch.io/tutorials/setup-a-react-environment-using-webpack-and-babel",
  "main": "client/index.js",
  "repository": "git@github.com:Malese/localreact.git",
  "author": "martin Lekvall <lekvall@gmail.com>",
  "license": "The Unlicense",
  "scripts": {
    "start": "webpack-dev-server",
    "dependency-check": "dependency-check ./package.json -e js,jsx --detective detective-cjs && dependency-check ./package.json -e js,jsx --detective detective-cjs --unused --no-dev",
    "test": "installed-check -e && eslint --ext .js --ext .jsx . && npm run dependency-check"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "sw-precache": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "dependency-check": "^2.8.0",
    "detective-cjs": "^2.0.0",
    "eslint": "^3.15.0",
    "eslint-config-semistandard": "^7.0.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-react": "^4.2.0",
    "eslint-plugin-promise": "^3.4.2",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-standard": "^2.0.1",
    "html-webpack-plugin": "^2.28.0",
    "installed-check": "^2.1.1",
    "path": "^0.12.7",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0"
  }
}
